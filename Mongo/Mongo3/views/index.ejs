<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mini Whatsapp</title>
    <!-- <style>
        .chat{

        }
    </style> -->
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <h3>All Chats</h3>

    <%for(let chat of chats){%>
        <div class="chat">
            <p>From : <%=chat.from%></p>
            <div class="msg">
                <p>Message : <%=chat.msg%></p>
            </div>
            <p>Recieved By<i> : <%=chat.to%></i></p>
        </div>
        <hr>
        <br>
    <%}%>

</body>
</html>

